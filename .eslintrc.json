{
  "env": { "node": true },
  "extends": "eslint:all",
  "parserOptions": { "ecmaVersion": 6 },
  "rules": {
    "dot-location": ["error", "property"],
    "indent": ["error", 2],
    "max-params": ["error", 4],
    "no-console": "off",
    "no-process-env": "off",
    "no-unused-vars": ["error", { "argsIgnorePattern": "^_" }],
    "one-var": ["error", "never"],
    "padded-blocks": ["error", "never"],
    "quotes": ["error", "single"]
  }
}
